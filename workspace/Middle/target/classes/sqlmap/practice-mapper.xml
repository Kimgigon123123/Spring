<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="practice">

<select id="select" resultType="practice.PracticeVO">
select * from crud_retrofit

</select>

<insert id="insert">
insert into crud_retrofit
values(seq_crud_retrofit.nextval,#{col1},'col2')
</insert>

<update id="update">
update crud_retrofit
set col1 = #{col1}

</update>

<delete id="delete">
delete crud_retrofit
where col_no = #{id}

</delete>

</mapper>